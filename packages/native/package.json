{
  "name": "empty-project-template",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "build": "expo build:android --non-interactive && expo build:ios --non-interactive",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "expo": "^30.0.1",
    "react": "^16.5.2",
    "@kjrocker/core": "*",
    "react-native": "https://github.com/expo/react-native/archive/sdk-30.0.0.tar.gz"
  },
  "devDependencies": {
    "@types/react": "^16.4.14",
    "@types/react-native": "^0.57.0",
    "typescript": "^3.4.3",
    "jest": "^23.6.0",
    "jest-expo": "^30.0.0",
    "ts-jest": "^23.10.2",
    "react-native-typescript-transformer": "^1.2.10"
  },
  "jest": {
    "preset": "jest-expo",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/?(*.)+(spec|test).ts?(x)"
    ]
  }
}
